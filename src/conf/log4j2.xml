<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="stderr" target="SYSTEM_ERR">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level [ %10thread ] %-58logger - %msg%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="stderr"/>
		</Root>
	</Loggers>
</Configuration>
